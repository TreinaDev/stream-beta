<%= form_with model: @user_profile do |f| %>
  <div>
    <%= f.label :full_name %>
    <%= f.text_field :full_name %>
  </div>
  <div>
    <%= f.label :social_name %>
    <%= f.text_field :social_name %>
  </div>
  <div>
    <%= f.label :birth_date %>
    <%= f.date_field :birth_date %>
  </div>
   <div>
    <%= f.label :cpf %>
    <%= f.text_field :cpf %>
  </div>
   <div>
    <%= f.label :zipcode %>
    <%= f.text_field :zipcode %>
  </div>
  <div>
    <%= f.label :address_line_one %>
    <%= f.text_field :address_line_one %>
  </div>
   <div>
    <%= f.label :address_line_two %>
    <%= f.text_field :address_line_two %>
  </div>
    <div>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>
    <div>
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>
    <div>
    <%= f.label :country %>
    <%= f.text_field :country %>
  </div>
  <%= f.submit %>
<% end %>
