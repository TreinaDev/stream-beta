<%= form_with model: @video do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :duration %>
    <%= f.text_field :duration %>
  </div>
  <div>
    <%= f.label :video_url %>
    <%= f.text_field :video_url %>
  </div>
  <div>
    <%= f.label :maturity_rating %>
    <%= f.text_field :maturity_rating %>
  </div>
  <%= f.submit %>
<% end %>
